{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true,
            "node": true
          }
        }
      ]
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "~": "./",
            "~~": "./src",
            "~services": "./src/services",
            "~utils": "./src/utils",
            "~orm": "./orm",
            "~public": "./public",
            "~externals": "./src/services/externals",
            "~sass": "./src/services/Saas"
          }
        }
      ]
    ]
  }